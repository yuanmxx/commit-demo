{
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qianduanxinlv/commit-ui.git"
  },
  "bugs": {
    "url": "https://github.com/qianduanxinlv/commit-ui/issues"
  },
  "version": "0.0.11",
  "license": "MIT",
  "name": "commit-ui",
  "homepage": "./",
  "description": "有趣的代码提交ui界面",
  "authors": {
    "name": "牧麟子",
    "email": "qianduanxinlv@163.com"
  },
  "keywords": [
    "commit",
    "funny",
    "git",
    "提交",
    "提交代码",
    "有趣的提交",
    "有趣的代码提交"
  ],
  "files": ["build", "README.md"],
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:ui": "react-scripts build",
    "build:server": "cd ../commit-build && yarn build:server && yarn copy-json:lib",
    "eject": "react-scripts eject",
    "serve": "node server/app.js",
    "clean": "node publish/clean.js",
    "test": "yarn tsc",
    "tsc": "tsc --incremental --diagnostics -p tsconfig.json",
    "switch": "node publish/index.js",
    "postpublish": "node publish/restore.js && git add . && git commit -m '人生何曾驻旧足，前端处处是新旅'",
    "push:ui": "cd ../commit-ui && git add . && git commit -m '人生何曾驻旧足，前端处处是新旅' && git push origin master",
    "push:server": "cd ../commit-server && git add . && git commit -m '人生何曾驻旧足，前端处处是新旅' && git push origin master"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
